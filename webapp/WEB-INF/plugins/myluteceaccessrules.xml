<?xml version="1.0" encoding="UTF-8"?><plug-in>
    <name>myluteceaccessrules</name>
    <class>fr.paris.lutece.plugins.myluteceaccessrules.service.MyluteceAccessRulesPlugin</class>
    <version>1.0.1</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>myluteceaccessrules.plugin.description</description>
    <provider>myluteceaccessrules.plugin.provider</provider>
    <provider-url>https://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2002-2021, City of Paris</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>4.1.0</min-core-version>
        <max-core-version/>
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>ACCESSRULES_MANAGEMENT</feature-id>
            <feature-title>myluteceaccessrules.adminFeature.ManageRules.name</feature-title>
            <feature-description>myluteceaccessrules.adminFeature.ManageRules.description</feature-description>
            <feature-level>0</feature-level>
            <feature-url>jsp/admin/plugins/myluteceaccessrules/ManageRules.jsp</feature-url>
            <feature-icon-url/>
        </admin-feature>
    </admin-features>

    <!-- XPages parameters -->
    <applications>
    </applications>

    <!-- Portlet parameters -->
    <portlets>
    </portlets>
    <filters>
		<filter>
			<filter-name>Mylutece Access Rules Filter</filter-name>
			<filter-class>fr.paris.lutece.plugins.myluteceaccessrules.web.AccessRulesFilter</filter-class>
			<url-pattern>/jsp/site/*</url-pattern>
			<filter-order>1000</filter-order>
		</filter>
	</filters>
</plug-in>